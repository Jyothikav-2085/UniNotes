{
  "name": "UniNotes",
  "version": "1.0.0",
  "description": "Run both frontend and backend concurrently",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start --prefix ./backend\" \"npm run dev --prefix ./frontend\"",
    "dev": "concurrently \"npm run start --prefix ./backend\" \"npm run dev --prefix ./frontend\"",
    "build": "npm run build --prefix ./frontend",
    "install-all": "npm install && npm install --prefix ./backend && npm install --prefix ./frontend"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.0.0"
  }
}